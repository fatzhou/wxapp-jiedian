/**app.wxss**/
@import './iconfont.wxss';
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  font-family: "Helvetica Neue",Helvetica,"PingFang SC","Hiragino Sans GB","Microsoft YaHei","微软雅黑",Arial,sans-serif;
} 

/* nodata start*/
.nodata-wrap .img-wrap {
    margin: 60px auto 0;
    width: 240rpx;
    height: 386rpx;
}

.nodata-wrap .img-wrap .img-inner {
    width: 100%;
    height: 100%;
}
/* nodata end*/
