/* pages/map/map.wxss */
.map-wrap{
  height: 100%;
  position: relative;
}
.map{
  width: 100%;
}
.search-wrap{
  padding: 0 32rpx;
  height: 96rpx;
  background-color: #000;
  display: flex;
}
.no-recharge {
  position: absolute;
  z-index:100;
  top: 120rpx;
  left: 5%;
  width: 90%;
  box-shadow: 2px 2px 1px #ccc;
  display: flex;
}
.no-recharge .tips {
  color: #df342a;
}
.no-recharge .recharge {
  color: #191919;
}
.user-info {
  width: 60rpx;
  height: 60rpx;
}
.user-info image {
  width: 100%;
  height: 100%;
}
.input-like{
  background-color: #fff;
  flex: 1;
  margin-top: 20rpx;
  height: 56rpx;
  border-radius: 32rpx;
  overflow: hidden;
  font-size: 14px;
  line-height: 56rpx;
  color: #c0c0c0;
}
.iconfont{
  font-size: 20px;
}
.icon-sousuo_sousuo{
  position: relative;
  margin-left: 16rpx;
  top: 4rpx;
}
.to-list{
  display: block;
  width: 56rpx;
  text-align: right;
  color: #fff;
  line-height: 96rpx;
}